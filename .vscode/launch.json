{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Angular",
            "type": "chrome",
            "request": "launch",
            "preLaunchTask": "npm: start",
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach to Node Functions",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "preLaunchTask": "func: host start"
        }
    ],
    "compounds": [
        {
            "name": "Jam Stack",
            "configurations": ["Launch Angular", "Attach to Node Functions"]
        }
    ]
}